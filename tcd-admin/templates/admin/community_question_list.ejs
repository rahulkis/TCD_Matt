<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Filter</h3>
            
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <form class="form-material" method="post">
                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Question</label>
                            <input type="text" name="question" placeholder="Question" class="form-control form-control-line" value="<%=data.filterDatas.question%>">
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label style="width:100%">&nbsp;</label>
                            <button type="submit" class="btn btn-primary" name="filter" value="1">Filter</button>
                            <button type="submit" class="btn btn-default" name="filter" value="2">Reset</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-header with-border">
                    <h3 class="box-title">Community Questions List</h3>
                </div>
                <div class="box-body">
                    <%- include ('./partials/admin_messages') %>
                    <table id="example2" class="table table-bordered table-striped">
                        <thead>
                            <tr class="headings">
								<th>Name</th>
								<th>Email</th>
                                <th>Question</th>
                                <th>Answer</th>
                                <th>Category</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody>
                        <%
                        if(data.communityQuestions && data.communityQuestions.length > 0){
                            data.communityQuestions.forEach((individualCommunityQuestion)=>{
                        %>
                            <tr id="id_<%= individualCommunityQuestion._id %>"> 
								<td><%=individualCommunityQuestion.full_name%></td>
								<td><%=individualCommunityQuestion.email%></td>
                                <td><%=individualCommunityQuestion.question%></td>
                                <td><%=individualCommunityQuestion.answer%></td>   
                                <td><%=individualCommunityQuestion.category_name %></td>                                
                  
                                <td>
                                    
                                    <a href="/admin/community-questions/update/<%= individualCommunityQuestion._id%>" title="Add Answer" class="btn btn-warning btn-xs" style="color:#fff"><i class="fa fa-plus"></i></a> ||
                                    <a href="/admin/community-questions/comments/<%= individualCommunityQuestion._id%>" title="View Comments" class="btn btn-warning btn-xs" style="color:#fff"><i class="fa fa-eye"></i></a> ||
                                    <a href="/admin/community-questions/delete/<%= individualCommunityQuestion._id%>" class="btn btn-danger btn-xs" style="color:#fff" onclick="return confirm('Are you sure,you want to delete this record ?')"><i class="fa fa-trash"></i></a> 
                                </td>
                            </tr>
                        <% })
                        } %>
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>
    <!-- /.row -->
</section>
<%- contentFor('page_js') %>