<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">Filter</h3>
                    <div class="box-tools pull-right">
                        <a href="/admin/partner/add" class="btn btn-primary">Add New Partner</a>
                        <button type="button" class="btn btn-box-tool" data-widget="collapse"><i
                                class="fa fa-minus"></i></button>
                    </div>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <form class="form-material" method="post">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Name</label>
                                    <input type="text" name="full_name" placeholder="Name"
                                        class="form-control form-control-line" value="<%=data.filterDatas.full_name%>">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Email</label>
                                    <input type="text" name="email" placeholder="Email"
                                        class="form-control form-control-line" value="<%=data.filterDatas.email%>">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Phone</label>
                                    <input type="text" name="contact_no" placeholder="Phone"
                                        class="form-control form-control-line" value="<%=data.filterDatas.contact_no%>">
                                </div>
                            </div>


                            <div class="col-md-3">
                                <div class="form-group">
                                    <label style="width:100%">&nbsp;</label>
                                    <button type="submit" class="btn btn-primary" name="filter"
                                        value="1">Filter</button>
                                    <button type="submit" class="btn btn-default" name="filter" value="2">Reset</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="box">
                <!-- /.box-header -->
                <div class="box-body">
                    <%- include ('./partials/admin_messages') %>
                        <table id="example2" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Partner Admin</th>
                                    <th>Phone</th>
                                    <th>Image</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(data.records && data.records.length> 0){
                                    data.records.forEach((user)=>{
                                    %>
                                    <tr>
                                        <td>
                                            <%=user.full_name%>
                                        </td>
                                        <td>
                                            <%=user.email%>
                                        </td>
                                        <td>
                                            <%=user.full_name%>
                                        </td>
                                        <td>
                                            <%=user.contact_no%>
                                        </td>
                                        <td>
                                            <% if(user.profile_image.length> 0){%>
                                                <span class="avatar-contact avatar-online"><img
                                                        src="https://tcd-project.s3.us-west-2.amazonaws.com/tcd-admin/profile_image/<%=user.profile_image%>"
                                                        alt="<%=user.name%>" height="50px" weight="50px"></span>
                                                <%}else{%>
                                                    <span class="avatar-contact avatar-online"><img
                                                            src="/images/avatar.png" alt="avatar" height="50px"
                                                            weight="50px"></span>
                                                    <%} %>

                                        </td>
                                        <td>
                                            <% if(user.is_active){ %>
                                                <span class="chip green lighten-5">
                                                    <span class="green-text">Active</span>
                                                </span>
                                                <% }else{ %>
                                                    <span class="chip red lighten-5">
                                                        <span class="red-text">Inactive</span>
                                                    </span>
                                                    <% } %>
                                        </td>
                                        <td>
                                            <!-- <a onclick="return getDetails('<%= user._id%>')" class="btn btn-primary btn-xs" style="color:#fff"><i class="fa fa-eye"></i></a> -->
                                            <!-- <a href="/admin/user-details/<%= user._id%>" class="btn btn-primary btn-xs" style="color:#fff"><i class="fa fa-eye"></i></a> -->
                                            <a href="/admin/partner/update/<%= user._id%>"
                                                class="btn btn-warning btn-xs" style="color:#fff"><i title="Edit"
                                                    class="fa fa-edit"></i></a>
                                            <!-- <button type="button" class="btn btn-danger btn-xs"><i class="fa fa-trash-o"></i></button> -->
                                            <% if(user.is_active==1){ %>
                                                <a href="/admin/partner/block-unblock/<%= user._id%>"
                                                    class="btn btn-info btn-xs blockusr"><i title="Block" class="fa fa-ban"></i></a>
                                                <% }else{ %>
                                                    <a href="/admin/partner/block-unblock/<%= user._id%>"
                                                        class="btn btn-info btn-xs"><i title="Unblock" class="fa fa-check"></i></a>
                                                    <% } %>
                                                        <a href="/admin/partner/delete/<%= user._id%>"
                                                            class="btn btn-danger btn-xs"
                                                            onclick="return confirm('Are you sure,you want to delete this record ?')"><i title="Delete"
                                                                class="fa fa-trash-o"></i></a>
                                        </td>
                                    </tr>
                                    <% }) } %>

                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Partner Admin</th>
                                    <th>Phone</th>
                                    <th>Image</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot>
                        </table>
                        <%- include('partials/pagination')%>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>
    <!-- /.row -->
</section>
<%- contentFor('page_js') %>