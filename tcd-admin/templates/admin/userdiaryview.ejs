<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
          
        <div class="col-md-12">
            
            <!-- Profile Image -->
            <div class="box box-primary">
                <div class="box-body box-profile">
    
                    <p class="text-muted text-center">User Diary Details</p>
					
					
                    <ul class="list-group list-group-unbordered">
                    <li class="list-group-item">
                        <b>User Name</b> <a class="pull-right"><%= data.userDiaryInfo.user.full_name%></a>
                    </li>
                    <li class="list-group-item">
                        <b>User Email</b> <a class="pull-right"><%= data.userDiaryInfo.user.email%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Created At</b> <a class="pull-right"><%= data.userDiaryInfo.created_at%>, <%= data.userDiaryInfo.day_of_week%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Name & Type of Product</b> <a class="pull-right"><%= data.userDiaryInfo.product.name%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Public Entry</b> <a class="pull-right"><%= data.userDiaryInfo.is_public == 1 ? 'Yes' : 'No'%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Strain</b> <a class="pull-right"><%= data.userDiaryInfo.product ? data.userDiaryInfo.product.strain.name : ''%></a>
                    </li>
					<li class="list-group-item">
                        <b>Ratings</b> <a class="pull-right"><% 
						for (let i = 1;i <= data.userDiaryInfo.average_ratings;i++) { %>
							<i class="fa fa-fw fa-star"></i>
						<% } %>
						</a>
                    </li>
                    <li class="list-group-item">
                        <b>Cannabinoid Profile</b> <a class="pull-right"><% 
														let cannabinoid_profile = ''
														data.userDiaryInfo.cannabinoid_profile.forEach((individualData) => {
															cannabinoid_profile += individualData.composition_id.name + ' - ' + individualData.weight + '%, '
														})								
														%>
													<%= cannabinoid_profile.slice(0, -2) %></a>
                    </li>
                    <li class="list-group-item">
                        <b>Terpenses</b> <a class="pull-right">
										<% 
										let terpenses = ''
										data.userDiaryInfo.terpenes.forEach((individualData) => {
											terpenses += individualData.composition_id.name + ' - ' + individualData.weight + '%, '
										})								
										%>
									<%= terpenses.slice(0, -2) %>
						</a>
                    </li>
					<li class="list-group-item">
                        <b>Symptoms</b> <a class="pull-right">
										<% 
								let symptoms = ''
								data.userDiaryInfo.pre_symptoms.forEach((individualData) => {
									symptoms += individualData.symptom_id.name + ', '
								})								
								%>
								<%= symptoms.slice(0, -2) %>
						</a>
                    </li>
                    <li class="list-group-item">
                        <b>Desired Effects</b> <a class="pull-right"><% 
								let deffects = ''
								data.userDiaryInfo.desired_effects.forEach((individualData) => {
									deffects += individualData.effect_id.name + ', '
								})								
								%>
								<%= deffects.slice(0, -2) %></a>
                    </li>
                    <li class="list-group-item">
                        <b>Actual Effects</b> <a class="pull-right"><% 
								let effects = ''
								data.userDiaryInfo.actual_effects.forEach((individualData) => {
									effects += individualData.effect_id.name + ', '
								})								
								%>
								<%= effects.slice(0, -2) %></a>
                    </li>
                    <li class="list-group-item">
                        <b>Activities</b> <a class="pull-right"><% 
								let activities = ''
								data.userDiaryInfo.pre_activities.forEach((individualData) => {
									activities += individualData.activity_id.name + ', '
								})								
								%>
								<%= activities.slice(0, -2) %></a>
                    </li>
                    <li class="list-group-item">
                        <b>Comments</b> <a class="pull-right"><%= data.userDiaryInfo.comments%></a>
                    </li>
					<li class="list-group-item">
                        <b>Consumption Method</b> <a class="pull-right"><%= data.userDiaryInfo.consumption_method ? data.userDiaryInfo.consumption_method.name : ''%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Eat Before Consumption</b> <a class="pull-right"><%= data.userDiaryInfo.eat_before_consumption%></a>
                    </li>
                    <li class="list-group-item">
                        <b>Consumption Time</b> <a class="pull-right"><%= data.userDiaryInfo.consumption_time%></a>
                    </li>
					<li class="list-group-item">
                        <b>Consumption Place</b> <a class="pull-right"><%= data.userDiaryInfo.consumption_place%></a>
                    </li>
					<li class="list-group-item">
                        <b>Consumption Partner</b> <a class="pull-right"><%= data.userDiaryInfo.consumption_partner%></a>
                    </li>
					<li class="list-group-item">
                        <b>Consumption Negative</b> <a class="pull-right"><%= data.userDiaryInfo.consumption_negative ? data.userDiaryInfo.consumption_negative.name : ''%></a>
                    </li>
					<li class="list-group-item">
                        <b>Mood Before Consumption</b> <a class="pull-right"><%= data.userDiaryInfo.mood_before_consumption ? data.userDiaryInfo.mood_before_consumption.name : ''%></a>
                    </li>
                    </ul>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
          
        
    </div>
    <!-- /.row -->
  
</section>
